programa
{
	
	funcao inicio()
	{
		inteiro numeros[5]
		inteiro indice

		para(indice = 0; indice < 5; indice ++){

			escreva("Informe o número para o índice [",indice,"]:")
			leia(numeros[indice])
			limpa()
				
		}

		limpa()

		para(indice = 0; indice < 5; indice ++){
			
			escreva("O número do índice [",indice,"] é: ",numeros[indice],"\n")
			
		}
		
	}
}
